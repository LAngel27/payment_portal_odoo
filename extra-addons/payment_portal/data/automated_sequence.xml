<odoo>
    <data noupdate="1">

        <record id="payment_reference_portal_payment_sequence" model="ir.sequence">
            <field name="name">Secuencia los registros de pagos referenciales provenientes del portal de pagos.</field>
            <field name="code">payment.reference.portal.payment</field>
            <field name="implementation">no_gap</field>
            <field name="active">True</field>
            <field name="company_id">1</field>
            <field name="prefix">PAY/REF/%(year)s/%(month)s/</field>
            <field name="padding">9</field>
            <field name="number_increment">1</field>
            <field name="use_date_range">True</field>
        </record>

    </data>
</odoo>
